{
  "publisher": "rubberduck",
  "name": "rubberduck-vscode",
  "version": "1.10.0",
  "displayName": "Rubberduck - ChatGPT for Visual Studio Code",
  "description": "Generate code, edit code, explain code, generate tests, find bugs, diagnose errors, and even create your own conversation templates.",
  "keywords": [
    "ai",
    "chatgpt",
    "gpt",
    "openai",
    "codex",
    "copilot",
    "code explanation"
  ],
  "categories": [
    "Machine Learning",
    "Programming Languages",
    "Linters",
    "Formatters",
    "Data Science",
    "Education",
    "Testing"
  ],
  "icon": "media/extension-icon.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "vscode": "^1.72.0"
  },
  "homepage": "https://github.com/rubberduck-ai/rubberduck-vscode",
  "repository": "https://github.com/rubberduck-ai/rubberduck-vscode",
  "bugs": "https://github.com/rubberduck-ai/rubberduck-vscode/issues",
  "extensionKind": [
    "workspace"
  ],
  "main": "./extension/dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "rubberduck.enterOpenAIApiKey",
        "category": "Rubberduck",
        "title": "Enter OpenAI API key"
      },
      {
        "command": "rubberduck.clearOpenAIApiKey",
        "category": "Rubberduck",
        "title": "Clear OpenAI API key"
      },
      {
        "command": "rubberduck.startChat",
        "title": "Start Chat ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "rubberduck.editCode",
        "title": "Edit Code ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "rubberduck.explainCode",
        "title": "Explain Code ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "rubberduck.generateCode",
        "title": "Generate Code ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(wand)"
      },
      {
        "command": "rubberduck.generateUnitTest",
        "title": "Generate Unit Test ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(beaker)"
      },
      {
        "command": "rubberduck.diagnoseErrors",
        "title": "Diagnose Errors ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(search-fuzzy)"
      },
      {
        "command": "rubberduck.findBugs",
        "title": "Find Bugs ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(bug)"
      },
      {
        "command": "rubberduck.startCustomChat",
        "title": "Start Custom Chatâ€¦ ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(comment-draft)"
      },
      {
        "command": "rubberduck.touchBar.startChat",
        "category": "Rubberduck",
        "title": "ðŸ’¬"
      },
      {
        "command": "rubberduck.showChatPanel",
        "title": "Show Chat Panel ðŸ’¬",
        "category": "Rubberduck",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "rubberduck.getStarted",
        "title": "Get Started",
        "category": "Rubberduck",
        "icon": "$(question)"
      },
      {
        "command": "rubberduck.reloadTemplates",
        "title": "Reload Templates",
        "category": "Rubberduck",
        "icon": "$(sync)"
      },
      {
        "command": "rubberduck.showLogs",
        "title": "Show Logs",
        "category": "Rubberduck",
        "icon": "$(output)"
      }
    ],
    "configuration": {
      "title": "Rubberduck",
      "properties": {
        "rubberduck.syntaxHighlighting.useVisualStudioCodeColors": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Use the Visual Studio Code Theme colors for syntax highlighting in the diff viewer. Might not work with all themes. Only applies to newly opened diffs.",
          "scope": "application"
        }
      }
    },
    "keybindings": [
      {
        "command": "rubberduck.startChat",
        "when": "isMac",
        "key": "Ctrl+Cmd+c"
      },
      {
        "command": "rubberduck.startChat",
        "when": "!isMac",
        "key": "Ctrl+Alt+c"
      },
      {
        "command": "rubberduck.editCode",
        "when": "isMac",
        "key": "Ctrl+Cmd+e"
      },
      {
        "command": "rubberduck.editCode",
        "when": "!isMac",
        "key": "Ctrl+Alt+e"
      },
      {
        "command": "rubberduck.generateCode",
        "when": "isMac",
        "key": "Ctrl+Cmd+g"
      },
      {
        "command": "rubberduck.generateCode",
        "when": "!isMac",
        "key": "Ctrl+Alt+g"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "rubberduck.startChat",
          "when": "view == rubberduck.chat",
          "group": "navigation@1"
        },
        {
          "command": "rubberduck.generateCode",
          "when": "view == rubberduck.chat",
          "group": "navigation@2"
        },
        {
          "command": "rubberduck.startCustomChat",
          "when": "view == rubberduck.chat",
          "group": "navigation@3"
        },
        {
          "command": "rubberduck.getStarted",
          "when": "view == rubberduck.chat",
          "group": "navigation@9"
        }
      ],
      "touchBar": [
        {
          "command": "rubberduck.touchBar.startChat",
          "group": "rubberduck"
        }
      ],
      "commandPalette": [
        {
          "command": "rubberduck.touchBar.startChat",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "rubberduck.startChat",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.editCode",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.explainCode",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.findBugs",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.generateUnitTest",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.diagnoseErrors",
          "group": "rubberduck",
          "when": "editorHasSelection"
        },
        {
          "command": "rubberduck.startCustomChat",
          "group": "rubberduck",
          "when": "editorHasSelection"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rubberduck",
          "title": "Rubberduck",
          "icon": "media/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "rubberduck": [
        {
          "id": "rubberduck.chat",
          "name": "Chat",
          "type": "webview"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "rubberduck",
        "title": "Rubberduck",
        "description": "Your AI Chat Assistant in Visual Studio Code",
        "steps": [
          {
            "id": "setup",
            "title": "Setup Rubberduck",
            "description": "[Enter your OpenAI API key](command:rubberduck.enterOpenAIApiKey) to get started.",
            "media": {
              "markdown": "walkthrough/setup.md"
            }
          },
          {
            "id": "chat",
            "title": "AI Chat",
            "description": "Chat with Rubberduck about your code and software development topics.",
            "media": {
              "markdown": "walkthrough/chat.md"
            }
          },
          {
            "id": "generate-code",
            "title": "Generate Code",
            "description": "Generate code by instructing Rubberduck.",
            "media": {
              "markdown": "walkthrough/generate-code.md"
            }
          },
          {
            "id": "edit-code",
            "title": "Edit Code",
            "description": "Change the selected code by instructing Rubberduck to create an edit.",
            "media": {
              "markdown": "walkthrough/edit-code.md"
            }
          },
          {
            "id": "explain-code",
            "title": "Explain Code",
            "description": "Ask Rubberduck to explain the selected code.",
            "media": {
              "markdown": "walkthrough/explain-code.md"
            }
          },
          {
            "id": "generate-unit-test",
            "title": "Generate Unit Tests",
            "description": "Generate unit tests with Rubberduck.",
            "media": {
              "markdown": "walkthrough/generate-unit-test.md"
            }
          },
          {
            "id": "find-bugs",
            "title": "Find Bugs",
            "description": "Identify potential defects in your code.",
            "media": {
              "markdown": "walkthrough/find-bugs.md"
            }
          },
          {
            "id": "diagnose-errors",
            "title": "Diagnose Errors",
            "description": "Let Rubberduck identify error causes and suggest fixes to fix compiler and linter errors faster.",
            "media": {
              "markdown": "walkthrough/diagnose-errors.md"
            }
          },
          {
            "id": "rubberduck-templates",
            "title": "Rubberduck Templates",
            "description": "Craft your own chat templates!",
            "media": {
              "markdown": "walkthrough/rubberduck-templates.md"
            }
          },
          {
            "id": "tips-and-tricks",
            "title": "Tips and Tricks",
            "description": "How to get the most out of Rubberduck.",
            "media": {
              "markdown": "walkthrough/tips-and-tricks.md"
            }
          }
        ]
      }
    ]
  }
}
