{
  "publisher": "rubberduck",
  "name": "rubberduck-vscode",
  "version": "0.0.5",
  "displayName": "Rubberduck - GPT-3 powered code chat",
  "description": "I'm building a chat assistant in public - follow along!",
  "keywords": [
    "ai",
    "gpt",
    "openai",
    "chatgpt",
    "codex",
    "copilot",
    "code explanation"
  ],
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters",
    "Data Science",
    "Machine Learning",
    "Education",
    "Testing"
  ],
  "icon": "media/extension-icon.png",
  "galleryBanner": {
    "color": "#111827",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "vscode": "^1.74.0"
  },
  "homepage": "https://github.com/rubberduck-ai/rubberduck-vscode",
  "repository": "https://github.com/rubberduck-ai/rubberduck-vscode",
  "bugs": "https://github.com/rubberduck-ai/rubberduck-vscode/issues",
  "extensionKind": [
    "workspace"
  ],
  "main": "./extension/dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "rubberduck.enterOpenAIApiKey",
        "category": "Rubberduck",
        "title": "Enter OpenAI API key"
      },
      {
        "command": "rubberduck.clearOpenAIApiKey",
        "category": "Rubberduck",
        "title": "Clear OpenAI API key"
      },
      {
        "command": "rubberduck.explainCode",
        "title": "Explain code",
        "category": "Rubberduck",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "rubberduck.writeTest",
        "title": "Write test",
        "category": "Rubberduck",
        "icon": "$(beaker)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rubberduck",
          "title": "Rubberduck",
          "icon": "media/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "rubberduck": [
        {
          "id": "rubberduck.chat",
          "name": "Chat",
          "type": "webview"
        }
      ]
    }
  }
}
